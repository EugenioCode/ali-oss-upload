{
  "name": "ali-oss-upload",
  "displayName": "ali-oss-upload",
  "description": "基于阿里云OSS上传图片的插件",
  "version": "0.0.3",
  "publisher": "jianjunyang",
  "repository": {
    "type": "git",
    "url": "https://github.com/EugenioCode/ali-oss-upload"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "enabledApiProposals": [
    "contribEditorContentMenu"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.chooseImage",
        "title": "选择图片"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "extension.chooseImage",
          "group": "navigation"
        }
      ]
    },
    "configuration": [
      {
        "title": "Ali Oss Upload",
        "properties": {
          "upload_oss_config.bucket": {
            "type": "string",
            "default": "",
            "description": "设置上传空间(必填)"
          },
          "upload_oss_config.region": {
            "type": "string",
            "default": "",
            "description": "地理区域(必填)"
          },
          "upload_oss_config.accessKeyId": {
            "type": "string",
            "default": "",
            "description": "设置AccessKeyId(必填)"
          },
          "upload_oss_config.accessKeySecret": {
            "type": "string",
            "default": "",
            "description": "设置AccessKeySecret(必填)"
          },
          "upload_oss_config.hostName": {
            "type": "string",
            "default": "",
            "description": "OSS域名"
          },
          "upload_oss_config.ossSavePath": {
            "type": "string",
            "default": "",
            "description": "保存路径"
          },
          "upload_oss_config.formatType": {
            "type": "string",
            "enum": ["url", "markdown","html"],
            "default": "url",
            "description": "图片输出格式"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/ali-oss": "^6.16.11",
    "ali-oss": "^6.19.0"
  }
}
